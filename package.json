{
  "name": "sharetribe-flex-sdk",
  "version": "1.4.0",
  "description": "Sharetribe Flex SDK for JavaScript",
  "main": "build/sharetribe-flex-sdk-node.js",
  "browser": "build/sharetribe-flex-sdk-web.js",
  "scripts": {
    "build": "webpack",
    "lint": "eslint .",
    "test": "jest",
    "repl": "babel-node repl.js",
    "format": "prettier --write --print-width 100 --single-quote --trailing-comma es5 'src/**/*.js' 'examples/**/*.js'",
    "serve-docs": "docpress serve",
    "build-docs": "docpress build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sharetribe/flex-sdk-js.git"
  },
  "keywords": [
    "sharetribe",
    "api",
    "sdk"
  ],
  "author": "Sharetribe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sharetribe/flex-sdk-js/issues"
  },
  "homepage": "https://github.com/sharetribe/flex-sdk-js",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "colors": "^1.3.1",
    "docpress": "^0.7.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "jest": "^23.4.2",
    "prettier": "^1.14.0",
    "repl.history": "^0.1.4",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "js-cookie": "^2.1.3",
    "lodash": "^4.17.10",
    "transit-js": "^0.8.861"
  },
  "babel": {
    "plugins": [
      "lodash"
    ],
    "presets": [
      "es2015",
      "stage-3"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
